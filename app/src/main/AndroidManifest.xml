<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.brunotonia.informatic">

    <application
        android:allowBackup="true"
        android:icon="@drawable/preferences_desktop"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".VIEW.PrincipalActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".VIEW.ClientesMenuActivity" />
        <activity android:name=".VIEW.OSMenuActivity" />
        <activity android:name=".VIEW.ClientesViewActivity" />
        <activity android:name=".VIEW.ClientesListarActivity"></activity>
    </application>

</manifest>